<div id="login_page">
<h1>Log in to</h1>
<form id="login" method="POST" action="/home">
	<div id="login_form">
		<div id="enter_username">
			<input id="enter_username" type="text" name="enter_username"placeholder="Username">
		</div>
		<div id="enter_password">
			<input id="password" type="text" name="enter_password"placeholder="Password">
		</div>
		<div id="login_box">
			<input id="login_button" type="submit" value="Log in"/>
		</div>
	</div>
</form>
</div>
